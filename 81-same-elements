public Boolean sameElements(List<Integer> nums1, List<Integer> nums2) {
   Set<Integer> s1 = new Set<Integer>();
   Set<Integer> s2 = new Set<Integer>();

   for(Integer i : nums1){
      s1.add(i); 
   }
   for(Integer i : nums2){
      s2.add(i); 
   }
   if(s1.size() == s2.size()){
       return true;
   }
   else {
       return false;
   }
}