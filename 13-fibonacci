public Integer fibonacci(Integer n) {
   Integer fOfZeroOrOne = 1;
   Integer fNminusTwo = 1;
   Integer fNminusOne = 1;  
   if (n == 0 || n == 1) {
      return fOfZeroOrOne;
   } else {
      for (Integer i = 2; i <= n; i++) {
         Integer temp = fNminusOne;
         fNminusOne = fNminusOne + fNminusTwo;
         fNminusTwo = temp;
      }
      return fNminusOne;
   }
}