public String aOrAn(String word) {
   String givenIp;
   word = word.toLowerCase();
    if (
            word.startsWith('a') || 
            word.startsWith('e') || 
            word.startsWith('i') || 
            word.startsWith('o') || 
            word.startsWith('u')
        ) {
      return  givenIp = 'An' + ' ' + word;
    } 
    else {
      return  givenIp = 'A' + ' ' + word;
    }
}