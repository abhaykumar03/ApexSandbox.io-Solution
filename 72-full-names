public List<String> fullNames(List<String> firstNames, List<String> lastNames) {
   List<String> fullNameList = new List<String>();
   if((firstNames.isNotEmpty() AND lastNames.isNotEmpty()) AND 
      firstNames.size() == lastNames.size()){
          for(Integer i = 0; i < firstNames.size(); i++){
            fullNameList fl = new fullNameList();
            fl = firstNames.get(i) + ' ' + lastNames.get(i);
            fullNameList.add(fl);
          }
      }
}