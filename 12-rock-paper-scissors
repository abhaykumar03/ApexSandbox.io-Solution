public Integer rockPaperScissors(String player1, String player2) {
    if(('rock','scissor') && ('paper','scissors')  && ('paper','rock'))
    return 1;
    else if(('scissor','rock') && ('scissors','paper')  && ('rock','paper'))
    return 2;
    else if(('rock','rock') && ('paper','paper') && ('scissors','scissors'))
    return 0;
}