public Integer rockPaperScissors(String player1, String player2) {
    if(('rock','scissors') ||
       ('scissors','paper') ||
       ('paper','rock')){
           return 1;
       }
    else if(('rock','paper') ||
            ('paper','scissors') ||
            ('scissors','rock')){
                return 2;
            }
    else(('rock','rock') ||
        ('paper','paper') ||
        ('scissors','scissors')){
            return 0;
        }
   
}